---
// Imports
import Base from './Base.astro';

import TestEntriesList from '../components/containers/TestEntriesList.jsx';

// Fetch all files in /pages/test-entries/
const testEntries = await Astro.glob('../pages/test-entries/*.md');
---
<Base title="test-entries">

  <TestEntriesList entries={testEntries} client:load />

</Base>
