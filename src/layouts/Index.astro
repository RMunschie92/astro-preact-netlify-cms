---
// Imports
import Base from './Base.astro';

import '../styles/layouts/Index.css';

import Index from '../components/containers/Index.jsx';

// The content prop gives access to frontmatter in a layout
const { content } = Astro.props;
---
<Base title="Index">
  <Index content={content} client:load>
    <slot />
  </Index>
</Base>